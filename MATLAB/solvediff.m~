function varargout = solvediff(Y,A,B,Y0)
%UNTITLED Summary of this function goes here
%   Detailed explanation goes here
odes = diff(Y) == A*Y + B;
initialcond=Y(0)==Y0;
if exist('Y0','var')
    Y = dsolve(odes,initialcond);
else
    Y = dsolve(odes);
end
for k = 1:nargout
      varargout{k} = simplify(Y(k));
end

